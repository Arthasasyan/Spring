<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
        "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
        "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="com.itmo.kotiki.entity.HumanEntity" table="human" schema="public" catalog="postgres">
        <id name="id">
            <column name="id" sql-type="integer"/>
        </id>
        <property name="name">
            <column name="name" sql-type="varchar(30)" length="30" not-null="true"/>
        </property>
        <property name="dateBirthday">
            <column name="date_birthday" sql-type="date" not-null="true"/>
        </property>
        <set name="catsById" inverse="true">
            <key>
                <column name="human_id" not-null="true"/>
            </key>
            <one-to-many not-found="ignore" class="com.itmo.kotiki.entity.CatsEntity"/>
        </set>
    </class>
</hibernate-mapping>