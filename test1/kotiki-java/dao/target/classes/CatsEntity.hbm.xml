<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
        "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
        "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="com.itmo.kotiki.entity.CatsEntity" table="cats" schema="public" catalog="postgres">
        <id name="id">
            <column name="id" sql-type="integer"/>
        </id>
        <property name="name">
            <column name="name" sql-type="varchar(30)" length="30" not-null="true"/>
        </property>
        <property name="dateBirthday">
            <column name="date_birthday" sql-type="date" not-null="true"/>
        </property>
        <property name="color">
            <column name="color" sql-type="varchar(30)" length="30" not-null="true"/>
        </property>
        <property name="breed">
            <column name="breed" sql-type="varchar(30)" length="30" not-null="true"/>
        </property>
        <many-to-one name="humanEntity" class="com.itmo.kotiki.entity.HumanEntity">
            <column name="human_id" not-null="true"/>
        </many-to-one>
    </class>
</hibernate-mapping>