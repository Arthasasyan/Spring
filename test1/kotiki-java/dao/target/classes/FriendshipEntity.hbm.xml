<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
        "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
        "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="com.itmo.kotiki.entity.FriendshipEntity" table="friendship" schema="public" catalog="postgres">
        <id name="id">
            <column name="id" sql-type="integer"/>
        </id>
        <property name="idOne">
            <column name="id_one" sql-type="integer" not-null="true"/>
        </property>
        <property name="idTwo">
            <column name="id_two" sql-type="integer" not-null="true"/>
        </property>
    </class>
</hibernate-mapping>